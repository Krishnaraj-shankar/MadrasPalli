

<div class="chapters">
    <h3>Class 10 - Social Science</h3>

    <br>


    {{#each sub}}

      <div class="accordion accordion-flush" id="accordionFlushExample">
  <div class="accordion-item">
    <div class="accordion-header">
      <div class="all-chap accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#flush-collapse{{add @index}}">
        <p class="chap-name">{{name}}</p>

        </div>
    </div>
    <div id="flush-collapse{{add @index}}" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
      <div class="accordion-body">
        {{#each chaps}}
          <a class="non-math-exercises {{add @index}}" data-id1="{{../name}}-{{add @index}}">
            <p>{{this}}</p>     
            {{!-- <img src="/images/chevron-r.svg" alt="arrow-right"> --}}
          </a>
        {{/each}}

      </div>
    </div>
  </div>
  
</div>

        
        

        
    {{/each}}
        
        
</div>