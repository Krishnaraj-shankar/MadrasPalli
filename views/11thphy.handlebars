

<div class="chapters">
    <h3>Class 11 - Physics</h3>

    <br>

    <p class="title-sm">Chapters</p>

    {{#each chapter}}

        <div class="accordion accordion-flush" id="accordionFlushExample">
  <div class="accordion-item">
      <div class="all-chap accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#flush-collapse{{add @index}}">
      <div class="chap-num">
          <p>{{add @index}}</p>
      </div>
        <p class="chap-name">{{this}}</p>

        </div>

    <div id="flush-collapse{{add @index}}" class="accordion-collapse collapse">
      <div class="accordion-body">
        
          <a class="non-math-exercises">
            <p data-id1="{{add @index}}">Topics & Explanation</p>     
            {{!-- <img src="/images/chevron-r.svg" alt="arrow-right"> --}}
          </a>
          <a class="non-math-exercises">
            <p data-id1="{{add @index}}">Example Problems</p>     
            {{!-- <img src="/images/chevron-r.svg" alt="arrow-right"> --}}
          </a>
          <a class="non-math-exercises">
            <p data-id1="{{add @index}}">Exercise Problems</p>     
            {{!-- <img src="/images/chevron-r.svg" alt="arrow-right"> --}}
          </a>
      </div>
    </div>
  </div>
  </div>

        

        
    {{/each}}
        
        
</div>




