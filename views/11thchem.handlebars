{{> nav}}

<div class="chapters">
    <h3>Class 11 - Chemistry</h3>

    <br>

    <p class="title-sm">Chapters</p>

    {{#each chapter}}

      <div class="accordion accordion-flush" id="accordionFlushExample">
  <div class="accordion-item">
    <div class="accordion-header">
      <div class="all-chap accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#flush-collapse{{@index}}">
      <div class="chap-num">
          <p>{{@index}}</p>
      </div>
        <p class="chap-name">{{this}}</p>

        </div>
    </div>
    <div id="flush-collapse{{@index}}" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
      <div class="accordion-body">
        <a class="non-math-exercises" href="">
            <p>Topics & Explanation</p>     
            <img src="/images/chevron-r.svg" alt="arrow-right">
          </a>
          <a class="non-math-exercises" href="">
            <p>Example Problems</p>     
            <img src="/images/chevron-r.svg" alt="arrow-right">
          </a>
          <a class="non-math-exercises" href="">
            <p>Exercise Problems</p>     
            <img src="/images/chevron-r.svg" alt="arrow-right">
          </a>

      </div>
    </div>
  </div>
  
</div>

        
        

        
    {{/each}}
        
        
</div>